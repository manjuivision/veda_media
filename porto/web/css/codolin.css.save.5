/* Style the Headings that are used to open and close the accordion panel */
 .product-details-main-acc-list .accordion {
  background-color: #eee;
  color: #444;
  cursor: pointer;
  padding: 18px;
  margin: 0;
  font-weight: 300;
}

/* Change color of the heading and icon (on hover and click) */
 .product-details-main-acc-list .active,.product-details-main-acc-list .accordion:hover, .accordion:hover::after {
  background-color: transparent;}

.product-details-main-acc-list .active,.product-details-main-acc-list .accordion, .accordion{font-weight: bold;font-size: 16.5px;background-color: transparent;color: #022169;padding-left: 0;position: relative;}

/* Add "plus" sign (+) after Accordion */
.product-details-main-acc-list .accordion::after {
  content: '\02038';
  color: #022169;
  float: right;
  font-size: 25px;
  position: absolute;
  right: 0;
  top: 20px;
  font-weight: normal;
  transform: rotate(180deg);
}

/* Add "minus" sign (-) after Accordion (when accordion is active) */
.product-details-main-acc-list .active::after {
  content: '\02038';
  color:#022169;
  font-size: 25px;
  transform: rotate(0);
  top: -15px;
}

/* Style the accordion panel */
.product-details-main-acc-list .accordion-panel {
  padding: 0 18px;
  overflow: hidden;
  max-height: 0;
  transition: max-height 0.2s ease-out;
  padding-left: 0;
  font-size: 14px;
  color: #5A5A5A;
}
.product-details-main-acc-list .product-details-accordia-sec:first-child{border-top: 2px solid #F5EAE7;}
